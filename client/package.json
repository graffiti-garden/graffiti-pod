{
  "name": "@graffiti-garden/implementation-federated",
  "version": "0.0.2",
  "description": "Federated implementation of the Graffiti API",
  "types": "./dist/client/src/index.d.ts",
  "module": "./dist/index.js",
  "browser": "./dist/index.browser.js",
  "main": "./dist/index.cjs.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/client/src/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/client/src/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "scripts": {
    "test": "vitest run --no-file-parallelism",
    "test:watch": "vitest --no-file-parallelism",
    "test:coverage": "vitest --no-file-parallelism --coverage",
    "build": "rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "prepublishOnly": "npm install && npm test && npm run build"
  },
  "author": "Theia Henderson",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@inrupt/solid-client-authn-node": "^2.3.0",
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.32.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "tslib": "^2.8.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@graffiti-garden/api": "^0.2.7",
    "@graffiti-garden/implementation-local": "^0.2.10",
    "@graffiti-garden/solid-oidc-session-manager": "^0.0.7",
    "@inrupt/solid-client": "^2.1.2",
    "ajv": "^8.17.1",
    "ajv-draft-04": "^1.0.0"
  }
}
